{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "outDir": "./.next",
    "declaration": false,
    "declarationDir": null,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules",
    ".next"
  ],
  "compileOnSave": false
}
