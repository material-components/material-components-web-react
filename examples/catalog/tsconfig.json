{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "lib": ["es2015", "es2017", "dom"],
    "jsx": "react",
    "target": "es5",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"],
  "compileOnSave": false
}
